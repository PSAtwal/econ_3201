#| label: tbl-dist
library(kableExtra)
d <- data.frame('\\hspace{1pt}' = c(rep('Value of X',3)),
'\\hspace{1pt}' = c(1,5,8),
'20' = c(0.07,0.32,0.05),
'30' = c(0.10,0.05,0.15),
'40' = c(0.04,0.03,0.19),
check.names = FALSE)
knitr::kable(d,
format = "latex",
booktabs = T,
escape = FALSE,
align = c("l","c","c","c","c"),
longtable= T )%>%
kable_styling(position = "center")%>%
add_header_above(c(" " = 1, " " = 1, "Value of Y" = 3))%>%
collapse_rows(columns = 1:2, valign = "top")
# Marginal probabilities for Y
marginal_Y <- colSums(d[, 3:5])
marginal_Y
y_values <- c(20, 30, 40)
mean_Y <- sum(y_values * marginal_Y)
mean_Y
variance_Y <- sum(y_values^2 * marginal_Y) - mean_Y^2
variance_Y
prob_a <- 1 - pnorm(1, mean = 3, sd = sqrt(9))
prob_a
prob_b <- pnorm(8, mean = 5, sd = sqrt(2)) - pnorm(6, mean = 5, sd = sqrt(2))
prob_b
#| echo: true
#| tbl-cap: "Joint Distribution of Covid-19 Vaccination Status and Hospitalizations Due to Covid-19"
#| label: tbl-vax
library(kableExtra)
d <- data.frame('\\hspace{1pt}' = c('Hospitalized',''),
'\\hspace{1pt}' = c('Yes','No'),
'Yes' = c(0.007,0.666),
'No' = c(0.075,0.252),
check.names = FALSE)
knitr::kable(d,
format = "latex",
booktabs = T,
escape = FALSE,
align = c("l","c","c","c"),
longtable= T)%>%
kable_styling(position = "center")%>%
add_header_above(c(" " = 1, " " = 1, "Vaccinated" = 2))
prob_vax_given_hosp <- 0.007 / (0.007 + 0.075)
prob_vax_given_hosp
pr_hosp <- 0.007 + 0.075  # P(Hospitalized = Yes)
pr_vax <- 0.007 + 0.666   # P(Vaccinated = Yes)
pr_hosp_given_vax <- (prob_vax_given_hosp * pr_hosp) / pr_vax
pr_hosp_given_vax
sample_mean <- 172
pop_mean <- 175
sample_sd <- 8
n <- 30
test_stat <- (sample_mean - pop_mean) / (sample_sd / sqrt(n))
test_stat
critical_value <- qnorm(0.975)  # For two-tailed test
critical_value
t_critical <- qt(0.975, df = n-1)
margin_error <- t_critical * (sample_sd / sqrt(n))
ci_lower <- sample_mean - margin_error
ci_upper <- sample_mean + margin_error
ci_lower
ci_upper
mean_treat <- 80
mean_control <- 75
sd_treat <- 10
sd_control <- 12
n <- 50  # Each group has 50 participants (100 total)
se <- sqrt((sd_treat^2/n) + (sd_control^2/n))
test_stat_7 <- (mean_treat - mean_control) / se
test_stat_7
critical_value_7 <- qnorm(0.95)
critical_value_7
# p-value
p_value_7 <- 1 - pnorm(test_stat_7)
p_value_7
